.user-settings
  height 100%
  overflow-y auto

.user-settings-themes
  display grid
  grid-template-columns repeat(3, 1fr)
  grid-template-rows repeat(auto-fit, 150px)
  gap 2%

.user-settings-theme
  height 150px
  border-radius 12px
  cursor pointer

.user-settings-tabs
  display flex
  
.user-settings-tab
  padding 4px 8px
  color white
  cursor pointer
